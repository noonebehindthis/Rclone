{
    "name": "Rclone",
    "description": "Bot to interface Heroku shell through Telegram with Rclone",
    "keywords": [
        "telegram",
        "shell",
        "rclone",
        "heroku"
    ],
    "repository": "https://github.com/leonalAbel/Rclone.git",
    "env": {
      "BOT_TOKEN": {
        "description": "The bot token",
        "required": true
      },
      "OWNER_ID": {
        "description": "Your Telegram user ID",
        "value": "",
        "required": true
      },
      "RCLONE_CONFIG_BASE64": {
        "description": "Your Rclone config encoded using Base64",
        "required": false
      }
    },
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      }
    ]
  }
